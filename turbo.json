{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "@coreproject-moe/icons-generator#build": {},
        "@coreproject-moe/icons#build": {
            "dependsOn": ["@coreproject-moe/icons-generator#build"]
        },
        "build": {
            "inputs": ["$TURBO_DEFAULT", ".env*"],
            "outputs": ["dist/**"],
            "dependsOn": ["^build"]
        },
        "test": {
            "outputs": ["coverage/**"],
            "dependsOn": []
        },
        "lint": {},
        "dev": {
            "cache": false,
            "persistent": true
        },
        "clean": {
            "cache": false
        }
    }
}
